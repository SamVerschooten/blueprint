{
  "name": "blueprint",
  "version": "0.0.2",
  "description": "Blueprint project useful to bootstrap new apps",
  "author": "Hatch Software",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SamVerschooten/blueprint.git"
  },
  "scripts": {
    "start": "npm run lint && npm run release && node dist/server",
    "release": "tsc && rimraf dist && webpack --config webpack/webpack.js --progress --colors --bail && node out/tools/build.js",
    "dev": "webpack-dev-server --config webpack/webpack.js --inline --progress --port 8080 --open",
    "e2e": "npm run tsc && ./node_modules/protractor/bin/webdriver-manager update && protractor out/protractor.conf.js",
    "lint": "./node_modules/tslint/bin/tslint 'src/**/*.ts?(x)' --force",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "dependencies": {
    "@angular/common": "~2.3.0",
    "@angular/compiler": "~2.3.0",
    "@angular/core": "~2.3.0",
    "@angular/forms": "~2.3.0",
    "@angular/http": "~2.3.0",
    "@angular/platform-browser": "~2.3.0",
    "@angular/platform-browser-dynamic": "~2.3.0",
    "@angular/router": "~3.3.0",
    "@angular/upgrade": "~2.3.0",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-rc.4",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.36",
    "@types/node": "^6.0.46",
    "@types/selenium-webdriver": "^2.53.33",
    "codelyzer": "^2.0.0-beta.4",
    "concurrently": "^2.2.0",
    "copy-webpack-plugin": "^3.0.1",
    "copyfiles": "^1.0.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "protractor": "^4.0.14",
    "node-sass": "^3.8.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "ts-loader": "^0.8.2",
    "tslint": "^4.3.1",
    "typescript": "^2.1.4",
    "url-loader": "^0.5.7",
    "watch": "^1.0.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": "^6.1.0",
    "npm": "^3.8.6"
  }
}
