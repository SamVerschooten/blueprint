{
  "name": "blueprint",
  "version": "0.0.1",
  "description": "Blueprint project useful to bootstrap new apps",
  "author": "Hatch Software",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SamVerschooten/blueprint.git"
  },
  "scripts": {
    "start": "npm run release && node dist/server",
    "release": "rimraf dist && webpack --config webpack/webpack.js --progress --colors --bail && node out/tools/build.js",
    "dev": "webpack-dev-server --config webpack/webpack.js --inline --progress --port 8080 --open",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "express": "^4.14.0",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23",
    "angular2-in-memory-web-api": "0.0.20",
    "bootstrap": "^3.3.6"
  },
  "devDependencies": {
    "angular2-jwt": "^0.1.18",
    "copy-webpack-plugin": "^3.0.1",
    "copyfiles": "^1.0.0",
    "foundation-sites": "^6.2.3",
    "html-webpack-plugin": "^2.22.0",
    "node-sass": "^3.8.0",
    "open-browser-webpack-plugin": "0.0.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "string-replace-webpack-plugin": "0.0.3",
    "ts-loader": "^0.8.2",
    "concurrently": "^2.2.0",
    "typescript": "^2.0.2",
    "typings":"^1.3.2",
    "watch": "^0.19.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  }
}
